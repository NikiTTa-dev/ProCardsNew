{
  "JwtSettings": {
    "Secret": "super-secret-key",
    "CookieName": ".AspNetCore.ProCards.Id",
    "ExpiryMinutes": 1,
    "Issuer": "ProCards",
    "Audience": "ProCards"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
